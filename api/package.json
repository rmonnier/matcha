{
  "name": "matcha",
  "version": "1.0.1",
  "description": "A dating web app",
  "main": "server.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/rmonnier/matcha"
  },
  "authors": "Robin Monnier",
  "scripts": {
    "start": "nodemon --inspect server.js",
    "test": "mocha --reporter spec",
    "eslint": "eslint **/*.js"
  },
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "~1.0.0",
    "cookie-parser": "~1.0.0",
    "dotenv": "^4.0.0",
    "express": "^4.15.2",
    "geolib": "^2.0.22",
    "jsonwebtoken": "^7.3.0",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "mongodb": "^2.2.26",
    "morgan": "^1.8.1",
    "multer": "^1.3.0",
    "nodemailer": "^4.0.1",
    "nodemon": "^1.11.0",
    "query-string": "^4.3.4",
    "sharp": "^0.18.1",
    "socket.io": "^1.7.3"
  },
  "devDependencies": {
    "chai": "^4.1.1",
    "eslint": "^4.4.1",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-plugin-chai-friendly": "^0.4.0",
    "mocha": "^3.5.0",
    "sinon": "^3.2.1",
    "sinon-mongoose": "^2.0.2",
    "supertest": "^3.0.0"
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "rules": {
      "comma-dangle": 0,
      "consistent-return": 0,
      "no-param-reassign": 0,
      "no-underscore-dangle": 0,
      "no-shadow": 0,
      "no-console": 0,
      "no-plusplus": 0,
      "no-unused-expressions": 0,
      "chai-friendly/no-unused-expressions": 2
    },
    "env": {
      "jasmine": true,
      "mocha": true,
      "node": true
    },
    "plugins": [
      "chai-friendly"
    ]
  }
}
